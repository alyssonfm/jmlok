<project name="jmlcCompiler" default="jmlc" basedir="..">
	<target name="jmlc">
		<echo>
             Compiling with JMLc compiler
        </echo>
		<exec command="${jmlcExec}">		
			<arg value="--recursive" />
			<arg value="${source_folder}" />
			
			<arg value="-d" />			
			<arg value="${jmlBin}" />
			
			<arg value="-G" />
			
		</exec>
	</target>
</project>